{
  "name": "<%= dasherizedBaseName %>",
  "version": "0.0.0",
  "description": "Description for <%= baseName %>",
  "private": true,
  "cacheDirectories": [
    "node_modules",
    "<%= MAIN_SRC_DIR %>bower_components"
  ],
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/upgrade": "2.0.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.10",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "core-js": "2.4.1",
    "jquery": "3.1.0",
    "angular2-cookie": "1.2.5",
    <%_ if (enableTranslation){ _%>
    "ng2-translate": "3.1.1",
    <%_ } _%>
    "ng2-webstorage": "1.3.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "ui-router-ng2": "1.0.0-beta.3",
    "ui-router-visualizer": "2.0.11",
    "webpack": "1.13.3",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "@types/angular": "1.5.16",
    "@types/angular-animate": "1.5.5",
    "@types/angular-mocks": "1.5.5",
    "@types/jasmine": "2.2.34",
    "angular": "1.5.8",
    "bower": "1.7.9",
    "browser-sync": "2.15.0",
    "del": "2.2.2",
    "eslint-config-angular": "0.5.0",
    "eslint-plugin-angular": "1.3.1",
    "event-stream": "3.3.4",
    "generator-jhipster": "<%= packagejs.version %>",
    "jasmine-core": "2.5.0",
    <%_ if (testFrameworks.indexOf('protractor') > -1) { _%>
    "jasmine-reporters": "2.1.1",
    <%_ } _%>
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-script-launcher": "1.0.0",
    "lazypipe": "1.0.1",
    "lodash": "4.15.0",
    "main-bower-files": "2.13.1",
    "map-stream": "0.0.6",
    "phantomjs-prebuilt": "2.1.12",
    <%_ if (testFrameworks.indexOf('protractor') > -1) { _%>
    "protractor": "3.3.0",
    "protractor-jasmine2-screenshot-reporter": "0.3.1",
    <%_ } _%>
    "proxy-middleware": "0.15.0",
    "run-sequence": "1.2.2",
    "typescript": "^2.0.3",
    <%_ if (buildTool == 'maven') { _%>
    "xml2js": "0.4.17",
    <%_ } _%>
    "yargs": "5.0.0"
  },
  "engines": {
    "node": "^4.3"
  },
  "scripts": {
    <%_ if (testFrameworks.indexOf('protractor') > -1) { _%>
    "postinstall": "webdriver-manager update",
    <%_ } _%>
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  }
}
